{
  "family":"ecs-getting-started-admin-panel",
  "networkMode":"awsvpc",
  "containerDefinitions":[
    {
      "name":"ecs-getting-started-admin-panel",
      "image":"<aws_account_id>.dkr.ecr.<region>.amazonaws.com/ecs-getting-started-admin-panel:latest",
      "portMappings":[
        {
          "containerPort":5000,
          "hostPort":5000,
          "protocol":"tcp"
        }
      ],
      "environment":[
        {
          "name":"NODE_ENV",
          "value":"production"
        },
        {
          "name":"PORT",
          "value":"5000"
        }
      ],
      "essential":true,
      "logConfiguration":{
        "logDriver":"awslogs",
        "options":{
          "awslogs-group":"/ecs/aws-ecs-getting-started",
          "awslogs-region":"<region>",
          "awslogs-stream-prefix":"awslogs-admin-panel"
        }
      },
      "entryPoint":[],
      "command":[]
    }
  ],
  "requiresCompatibilities":[
    "FARGATE"
  ],
  "cpu":"256",
  "memory":"512",
  "executionRoleArn": "arn:aws:iam::<aws_account_id>:role/ecsTaskExecutionRole"
}